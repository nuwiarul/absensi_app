{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n              id,\n              scope as \"scope: HolidayScope\",\n              satker_id,\n              holiday_date,\n              kind as \"kind: HolidayKind\",\n              name,\n              half_day_end\n            FROM holidays\n            WHERE holiday_date BETWEEN $1 AND $2\n              AND (\n                scope = 'NATIONAL'\n                OR (scope = 'SATKER' AND satker_id = $3)\n              )\n            ORDER BY holiday_date ASC\n            ",
